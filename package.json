{
  "name": "@datasigninc/react-native-did-siop",
  "version": "0.6.2",
  "repository": "git://github.com/DataSignInc/react-native-did-siop",
  "description": "A React Native library for did-siop v2 (OpenID Connect Self-Issued Identity Provider for Decentralized Identities). Only including Provider funcionality.",
  "main": "./dist/index.js",
  "react-native": "dist/index.js",
  "scripts": {
    "test": "jest",
    "build": "tsc",
    "watch": "tsc --watch",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "generate-json-schema": "npx typescript-json-validator src/siop-schema.d.ts Registration",
    "ajv:compile": "ajv compile -s src/registration.json -o src/ajv-schemas/registration.js"
  },
  "keywords": [
    "Self_Severign_Identity",
    "DID",
    "SIOP",
    "Self_Issued_OpenIDConnect_Provider",
    "DID_SIOP",
    "OpenIDConnect",
    "React_Native"
  ],
  "author": "DataSign Inc.",
  "license": "MIT",
  "dependencies": {
    "ajv": "^6.5.5",
    "buffer": "^6.0.3",
    "crypto-js": "3.3.0",
    "did-jwt": "^5.1.2",
    "did-resolver": "^3.1.0",
    "elliptic": "^6.5.3",
    "query-string": "^7.0.0"
  },
  "peerDependencies": {
    "react-native": "*"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/runtime": "^7.8.4",
    "@react-native-community/eslint-config": "^1.1.0",
    "@types/crypto-js": "^3.1.47",
    "@types/elliptic": "^6.4.12",
    "@types/jest": "^25.2.3",
    "@types/react-native": "^0.63.2",
    "ajv-cli": "3.3.0",
    "babel-jest": "^25.1.0",
    "eslint": "^6.5.1",
    "ethr-did-resolver": "^4.3.4",
    "jest": "^25.1.0",
    "jest-fetch-mock": "^3.0.3",
    "metro-react-native-babel-preset": "^0.59.0",
    "prettier": "^2.2.1",
    "prettier-eslint": "^12.0.0",
    "react-native": "^0.64.0",
    "typescript": "^3.8.3",
    "web-did-resolver": "^2.0.4"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "testPathIgnorePatterns": [
      "./__tests__/consts.ts",
      "./node_modules",
      "./dist",
      ".*\\.d\\.validator\\.ts"
    ],
    "verbose": false
  }
}